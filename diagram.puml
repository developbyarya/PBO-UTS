    + menghitungVolume(params: int[]): double
    + menghitungLuasPermukaan(): float
    + menghitungLuasPermukaan(params: double[]): double
    + menghitungLuasPermukaan(params: int[]): d ouble
} 

abstract class Benda2D {
  - nama: String
  - warna: String
